<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TestTheme" parent="AppTheme">
        <item name="android:enforceNavigationBarContrast" tools:ignore="NewApi">false</item>
    </style>

    <style name="CommonDialog" parent="Theme.AppCompat.DayNight.Dialog" tools:ignore="NewApi">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimAmount">0.5</item>
        <item name="android:forceDarkAllowed" >false</item><!--禁止系统自动转色app-->
    </style>

</resources>
